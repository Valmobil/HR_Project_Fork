        <label>Selected Topic</label>
        <locationX>1428</locationX>
        <locationY>715</locationY>
        <assignNextValueToReference>currentItem_Selected_Topic</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>Filter_Topics</collectionReference>
        <conditionLogic>and</conditionLogic>
        <conditions>
            <leftValueReference>currentItem_Selected_Topic.Id</leftValueReference>
            <operator>EqualTo</operator>
            <rightValue>
                <elementReference>Topics</elementReference>
            </rightValue>
        </conditions>
        <connector>
            <targetReference>Skip_Selected_Topic</targetReference>
        </connector>
    </collectionProcessors>
