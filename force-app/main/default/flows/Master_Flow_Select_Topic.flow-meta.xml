    <collectionProcessors>
        <description>Filters only relevant topics based which vacancy type matches the vacancy type on the vacancy.</description>
        <name>Filter_Topics</name>
        <elementSubtype>FilterCollectionProcessor</elementSubtype>
        <label>Filter Topics</label>
        <locationX>745</locationX>
        <locationY>539</locationY>
        <assignNextValueToReference>currentItem_Filter_Topics</assignNextValueToReference>
        <collectionProcessorType>FilterCollectionProcessor</collectionProcessorType>
        <collectionReference>Get_Topics</collectionReference>
        <conditionLogic>and</conditionLogic>
        <conditions>
            <leftValueReference>currentItem_Filter_Topics.Vacancy_Type__c</leftValueReference>
            <operator>Contains</operator>
            <rightValue>
                <elementReference>Get_Contacts.Account.Vacancy_Type__c</elementReference>
            </rightValue>
        </conditions>
        <connector>
            <targetReference>check_if_no_topics</targetReference>
        </connector>
    </collectionProcessors>
    <collectionProcessors>
        <description>Filters Record Collection Variable - Interview Question Topics from Filter_Topic to show only the topic which was selected on the previous screen.</description>
        <name>Selected_Topic</name>
